<div class="container">
  <div class="row">
    <div class="col-8 offset-2">
      <div class="card mt-4">
        <div class="card-body">
          <h5 class="card-title">{{title}}</h5>
          <p>
            <strong>Article ID</strong>: {{id}}
          </p>

          <p>
            <strong>ArticleLink</strong>:<a href="#" data-link="{{link}}" data-id="{{id}}" class="modal-link">
              {{link}}</a>
          </p>
          {{#each notes}}
          <p>
            <h4>Note</h4>
          </p>
          <p><strong>{{title}}</strong></p>
          <p>{{body}}</p>
          <p>
            <button type="button" class="btn btn-primary delete-btn" data-artid="{{id}}" data-id="{{_id}}">Delete Note</button>
          </p>
          {{/each}}


          <a href="/">‚Üê Back To Home</a>
        </div>
      </div>
    </div>
  </div>
</div>

